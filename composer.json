{
    "name": "bkstg/core-bundle",
    "description": "Provides the core backstage functionality and requirements.",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/orm": "^2.5",
        "exercise/htmlpurifier-bundle": "^1.0",
        "friendsofsymfony/ckeditor-bundle": "^1.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "knplabs/knp-paginator-bundle": "^2.6",
        "midnightluke/group-security-bundle": "^1.0@dev",
        "sonata-project/block-bundle": "^3.3",
        "sonata-project/media-bundle": "^3.6",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/framework-bundle": "^3.3||^4.0",
        "symfony/security-bundle": "^3.3||^4.0",
        "symfony/twig-bundle": "^3.3||^4.0",
        "twig/extensions": "^1.5",
        "symfony/translation": "^4.0"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.69",
        "foxy/foxy": "^1.0",
        "kint-php/kint": "^3.0",
        "phpunit/phpunit": "^7.4"
    },
    "authors": [
        {
            "name": "Luke Bainbridge",
            "email": "luke.bainbridge@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": { "Bkstg\\CoreBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
